<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/orange"
    tools:context=".MainActivity" >

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


   <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    style="@style/info_layout">


        <TextView android:id="@+id/caption"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/Scanning_text" />

        <LinearLayout style="@style/entry_layout">
            <Button android:id="@+id/start_gscan_btn"
                android:layout_below="@+id/caption"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/start_gscan_bt" />

            <Button android:id="@+id/stop_gscan_btn"
                android:layout_below="@+id/start_gscan_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/stop_gscan_bt" />
           <Button android:id="@+id/get_gscan_result_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/get_gscan_result_bt" />
        </LinearLayout>

        <TextView android:id="@+id/scansettings"
            android:layout_below="@+id/stop_gscan_btn"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/scansettings_txt1" />

        <TextView android:id="@+id/scansettings2"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/scansettings_txt2" />

       <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/supportset_lbl" android:text="@string/supportset_txt" style="@style/info_label" />
            <EditText android:id="@+id/supportset_value" style="@style/info_value" android:inputType="number"/>
        </LinearLayout>


       <LinearLayout style="@style/entry_layout">
           <TextView android:id="@+id/title1" android:text="@string/set1" style="@style/info_label" />
           <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
           <EditText android:id="@+id/set1_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
        	<TextView android:id="@+id/title1" android:text="@string/set1" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set1_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>


       <LinearLayout style="@style/entry_layout">
        <TextView android:id="@+id/title2" android:text="@string/set2" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set2_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
         <TextView android:id="@+id/title2" android:text="@string/set2" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set2_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

       <LinearLayout style="@style/entry_layout">
        <TextView android:id="@+id/title3" android:text="@string/set3" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set3_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
         <TextView android:id="@+id/title3" android:text="@string/set3" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set3_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>


       <LinearLayout style="@style/entry_layout">
       	<TextView android:id="@+id/title4" android:text="@string/set4" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set4_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
        	<TextView android:id="@+id/title4" android:text="@string/set4" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set4_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>


       <LinearLayout style="@style/entry_layout">
       	<TextView android:id="@+id/title5" android:text="@string/set5" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set5_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
        	<TextView android:id="@+id/title5" android:text="@string/set5" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set5_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>
        
       <LinearLayout style="@style/entry_layout">
       	<TextView android:id="@+id/title6" android:text="@string/set6" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set6_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
        	<TextView android:id="@+id/title6" android:text="@string/set6" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set6_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>
 
       <LinearLayout style="@style/entry_layout">
       	<TextView android:id="@+id/title7" android:text="@string/set7" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set7_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
        	<TextView android:id="@+id/title7" android:text="@string/set7" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set7_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>
        
       <LinearLayout style="@style/entry_layout">
       	<TextView android:id="@+id/title8" android:text="@string/set8" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt1_lbl" android:text="@string/gscan_txt1" style="@style/info_label" />
            <EditText android:id="@+id/set8_value1" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
        	<TextView android:id="@+id/title8" android:text="@string/set8" style="@style/info_label" />
            <TextView android:id="@+id/channel1_txt_lbl2" android:text="@string/gscan_txt2" style="@style/info_label" />
            <EditText android:id="@+id/set8_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/scan_list" android:text="@string/scan_list_label" style="@style/info_label" />
        </LinearLayout>

        <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>


        <TextView android:id="@+id/wifi_tracking_change_caption"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/wifi_tracking_change_caption_txt" />

        <LinearLayout style="@style/entry_layout">
           <Button android:id="@+id/start_track_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/start_track_bt" />
           <Button android:id="@+id/stop_track_btn"
                android:layout_below="@+id/start_track_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/stop_track_bt" />

           <Button android:id="@+id/set_wifichange_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/set_wifichange_bt" />
        </LinearLayout>

        <TextView android:id="@+id/wifi_change_lbl"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/wifi_change_txt" />

            <TextView android:id="@+id/wifi_change_lbl" android:text="@string/channel1_txt1" style="@style/info_label" />
            <EditText android:id="@+id/wifi_change_value" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>
            <TextView android:id="@+id/wifi_change_lbl2" android:text="@string/wifi_change_txt2" style="@style/info_label" />
            <EditText android:id="@+id/wifi_change_value2" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>

        <TextView android:id="@+id/caption_onchange"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/on_change_list_lbl" />

        <TextView android:id="@+id/on_change_list" android:text="@string/on_change_list_lbl" style="@style/info_label" />

        <TextView android:id="@+id/caption_onquiescence"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/on_mquiescence_list_lbl" />

        <LinearLayout style="@style/entry_layout">
        <TextView android:id="@+id/on_mquiescence_list" android:text="@string/on_mquiescence_list_lbl" style="@style/info_label" />
        </LinearLayout>

        <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>


        <TextView android:id="@+id/hotlist_caption_lbl"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/hotlist_caption_text" />

        <LinearLayout style="@style/entry_layout">
           <Button android:id="@+id/start_hotlist_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/start_hotlist_bt" />

           <Button android:id="@+id/stop_hotlist_btn"
               android:layout_width="wrap_content" android:layout_height="wrap_content"
               android:text="@string/stop_hotlist_bt" />
        </LinearLayout>

        <TextView android:id="@+id/hotlist_lbl" android:text="@string/hotlist_txt" style="@style/info_label" />
        <EditText android:id="@+id/hotlist_value" style="@style/info_value" android:inputType="textNoSuggestions|textCapCharacters"/>

        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/hotlist_list" android:text="@string/hotlist_list_label" style="@style/info_label" />
        </LinearLayout>


        <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>

        <TextView android:id="@+id/RTT_text_lbl"
            android:layout_below="@+id/stop_hotlist_btn"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/RTT_text" />

        <LinearLayout style="@style/entry_layout">
            <Button android:id="@+id/start_ranging_btn"
                android:layout_below="@+id/RTT_text_lbl"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/start_ranging_bt" />

           <Button android:id="@+id/stop_ranging_btn"
                android:layout_below="@+id/start_ranging_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/stop_ranging_bt" />
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/wifi_rtt_result" style="@style/info_value" />
        </LinearLayout>

        <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>

        <TextView android:id="@+id/linklayer_text_lbl"
            android:layout_below="@+id/stop_ranging_btn"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="@string/linklayer_text" />

        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/lls_list" android:text="@string/lls_result" style="@style/info_label" />
        </LinearLayout>

 <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>

        <!--Wi-Fi Capabilities-->
        <LinearLayout style="@style/entry_layout">
            <Button android:id="@+id/wificap_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/wifi_capability" />
        </LinearLayout>
        <LinearLayout style="@style/entry_layout">
            <TextView android:text="@string/wifi_capability" style="@style/info_label" />
        </LinearLayout>
        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/wifi_capability" style="@style/info_value" />
        </LinearLayout>
 <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>


        <!--Wi-Fi Avaliable channel-->
        <LinearLayout style="@style/entry_layout">
            <Button android:id="@+id/wifiavalchannel_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/wifi_avaliable_channel" />
            <TextView android:id="@+id/band_lbl" android:text="@string/band_txt" style="@style/info_label" />
            <EditText android:id="@+id/band_value" style="@style/info_value" android:inputType="number"/>
        </LinearLayout>

        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/wifi_avaliable_channel" style="@style/info_value" />
        </LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#777777"></View>
        <!--Wi-Fi Passpoint-->
        <LinearLayout style="@style/entry_layout">
           <Button android:id="@+id/wifihs20_btn"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="@string/wifi_hs20" />
        </LinearLayout>
        <LinearLayout style="@style/entry_layout">
            <TextView android:text="@string/wifi_hs20" style="@style/info_label" />
        </LinearLayout>
        <LinearLayout style="@style/entry_layout">
            <TextView android:id="@+id/wifi_hs20" style="@style/info_value" />
        </LinearLayout>

</LinearLayout>
</ScrollView>

</RelativeLayout>
