<ft_testcase_conditions>
    <testcase_chapter id="1" name="基本性能测试" phone_type="common">
        <testcase id="1.1.1" name="&quot;差点&quot;开机附着成功率" case_type="static">
            <condition descPrefix="SINR " descSeparator="~" descSuffix="dB">
                <check_item_name>SINR</check_item_name>
                <check_type>value_between</check_type>
                <setting_view_type>EditText_2</setting_view_type>
                <value_left>-3</value_left>
                <value_right>0</value_right>
                <judge_type>V</judge_type>
            </condition>
            <condition descPrefix="RSRP " descSeparator="~" descSuffix="dBm">
                <check_item_name>RSRP</check_item_name>
                <check_type>value_between</check_type>
                <setting_view_type>EditText_2</setting_view_type>
                <value_left>-120</value_left>
                <value_right>-110</value_right>
                <judge_type>V</judge_type>
            </condition>
        </testcase>
